@model List<Treat>

<h2>Welcome to Pierre's Sweet and Savory Treats</h2>
<div>
    <h3>Treats</h3>
    <ul>
        @foreach (var treat in Model)
        {
            <li>@treat.Name</li>
        }
    </ul>
</div>
<div>
    <h3>Flavors</h3>
    <ul>
        @foreach (var flavor in ViewBag.Flavors)
        {
            <li>@flavor.Name</li>
        }
    </ul>
</div>
